<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.ssrdi.co.id.myradboox.fragmentreseller.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_detailVoucherFragment"
            app:destination="@id/detailVoucherFragment" />
    </fragment>
    <fragment
        android:id="@+id/detailVoucherFragment"
        android:name="com.ssrdi.co.id.myradboox.fragmentreseller.DetailVoucherFragment"
        android:label="fragment_detail_voucher"
        tools:layout="@layout/fragment_detail_voucher" />
    <fragment
        android:id="@+id/generateFragment"
        android:name="com.ssrdi.co.id.myradboox.fragmentreseller.GenerateFragment"
        android:label="fragment_generate"
        tools:layout="@layout/fragment_generate" >
        <action
            android:id="@+id/action_generateFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
</navigation>